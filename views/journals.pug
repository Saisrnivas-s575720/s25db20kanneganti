extends layout

block content
  h1 Search Results: Journals

  -
    var results = [
      { title: "Journal of Astrophysics", field: "Astronomy", impactFactor: 8.2 },
      { title: "Machine Learning Review", field: "Artificial Intelligence", impactFactor: 12.7 },
      { title: "Global Climate Reports", field: "Environmental Science", impactFactor: 6.5 }
    ];

  .container-fluid
    .row
    
      each journal in results
        .col-sm-2.bg-light.border.p-2.m-2.rounded
          p Title: #{journal.title}
          p Field: #{journal.field}
          p Impact Factor: #{journal.impactFactor}
